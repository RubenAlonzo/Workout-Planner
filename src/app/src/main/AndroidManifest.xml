<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workoutplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Home"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WorkoutPlanner">
        <activity android:name=".NewExerciseActivity"
            android:label="New exercise"
            android:parentActivityName=".ExercisesActivity"></activity>
        <activity
            android:name=".ExercisesActivity"
            android:label="Exercises"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".RoutinesActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>